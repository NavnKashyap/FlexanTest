<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.test.ecommercart">

    <application
        android:allowBackup="false"
        android:icon="@drawable/ic_cart"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_cart"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.test.ecommercart.MainActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.test.ecommercart.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.test.ecommercart.CartActivity" />
        <activity android:name="com.test.ecommercart.OrdersActivity" />
    </application>

</manifest>